{
  "name": "react-forms",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "container": "sudo docker-compose up --build -d ui",
    "containers-start": "sudo docker-compose up --build -d",
    "containers-stop": "sudo docker-compose down -v --remove-orphans",
    "dev": "vite --port 3010 --host",
    "build": "tsc && vite build",
    "build-force": "tsc-silent -p './tsconfig.json' --suppress @ && vite build",
    "preview": "vite preview --port 3010 --host"
  },
  "dependencies": {
    "@dinero.js/currencies": "2.0.0-alpha.11",
    "@rsuite/icons": "^1.0.2",
    "axios": "^1.2.0",
    "bootstrap": "^5.2.3",
    "currency.js": "^2.0.4",
    "date-fns": "^2.29.3",
    "dinero.js": "2.0.0-alpha.11",
    "jquery": "^3.6.1",
    "react": "^18.2.0",
    "react-currency-masked-input": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-json-tree": "^0.17.0",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.4.5",
    "react-text-mask": "^5.5.0",
    "react-toastify": "^9.1.1",
    "rsuite": "^5.23.0",
    "text-mask-addons": "^3.8.0",
    "tsc-silent": "^1.2.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
